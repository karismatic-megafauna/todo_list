<h1>Login</h1>

<%= form_tag session_path do %>
  <div class="field">
    <%= label_tag :email %><br>
    <%= text_field_tag :email %>
  </div>

  <div class="field">
    <%= label_tag :password %><br>
    <%= text_field_tag :password %>  
  </div>
  
  <div class="actions">
    <%= submit_tag 'Login' %>  
  </div>
<% end %>
<span class="button">
  <%= link_to "Create New Account", new_user_path %>
</span>
